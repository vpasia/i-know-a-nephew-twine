:: StoryStylesheet [stylesheet]
@import url('https://fonts.googleapis.com/css2?family=Exo:ital,wght@0,100..900;1,100..900&display=swap');

tw-story {
  font-family: 'Exo', sans-serif;
  font-weight: bold;  
}

tw-story[tags~="explore"] {
  background-image:url('./assets/images/explore.png');
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;
}

tw-story[tags~="silent"] {
  background-image:url('./assets/images/silent.gif');
  background-repeat: no-repeat;
  background-size: 80% 100%;
  background-position: center center;
}

tw-story[tags~="confront"] {
  text-shadow: 0 0 5px #cb4b16;
  background-image:url('./assets/images/confront.png');
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center center;

}

tw-story[tags~="reflection"] {
  text-shadow: 0 0 5px #cb4b16;
  background-image:url('./assets/images/reflection.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-position: center center;
}

tw-story[tags~="hr"] {
  background-image:url('./assets/images/hr.png');
  background-repeat: no-repeat;
  background-size: 80% 100%;
  background-position: center center;
}

tw-story[tags~="bloodline"] {
  background-image:url('./assets/images/bloodline.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-position: center center;
}

tw-story[tags~="empty"] {
  background-image:url('./assets/images/empty.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-position: center center;
}

tw-story[tags~="files"] {
  background-image:url('./assets/images/files.png');
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center center;
}

tw-story[tags~="face"] {
  background-image:url('./assets/images/face.png');
  background-repeat: no-repeat;
  background-size: contain;
  background-position: center center;
}

tw-passage img {
	display: block;
	margin: 0 auto;
}

.container {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.text {
  margin-bottom: 25px;
}

.loading {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.loading .line-box {
  padding: 3px;
  width: 40%;
  height: 30px;
  border: 2px solid white;
  border-radius: 20px;
}

.loading .line-box .line {
  height: 20px;
  border-radius: 20px;
  background: white;
  animation: loading 4s forwards cubic-bezier(0,0,0,0);
}

@keyframes loading {
  0%   {width: 0%}
  25%  {width: 25%}
  50%  {width: 50%}
  75%  {width: 75%}
  100% {width: 100%}
}

.enchantment-link, tw-link, a {
	color: white; 
	font-weight: bold;
  	font-size: 1.5em;
	text-shadow: 0 0 5px #cb4b16;
}

.enchantment-link:hover, tw-link:hover, a:hover {
	color: red; 
	font-weight: bold; 
  	font-size: 1.5em;
	text-shadow: 0 0 5px #2aa198;
}

.enchantment-clickblock > :not(tw-enchantment)::after {
	box-shadow: none;
}

